package com.ait.sysparam;


/**
 * 处理系统参数引发的异常
 * @version 1.0
 */
public class SysparamException extends RuntimeException{
	

    public SysparamException() {
	   super();
    }

    public SysparamException(String message) {
	   super(message);
    }

    public SysparamException(String message, Throwable cause) {
        super(message, cause);
    }

    public SysparamException(Throwable cause) {
           super(cause);
    }

	private static final long serialVersionUID = -5609872286821389726L;

}
