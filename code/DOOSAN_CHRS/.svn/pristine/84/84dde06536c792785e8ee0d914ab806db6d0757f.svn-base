function CheckForm() {

		if (document.sf.param_name.value == "") 	{
			alert ("\n 请填写param_name！");
			document.sf.param_name.focus();
			return false;
		}

		if (document.sf.param_name.value.length > 25) 	{
			alert ("\n param_name字数不能大于25，请修改！");
			document.sf.param_name.focus();
			return false;
		}		

		return true;
}
