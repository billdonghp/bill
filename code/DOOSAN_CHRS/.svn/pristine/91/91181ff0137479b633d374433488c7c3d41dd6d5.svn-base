package com.ait.evs;

import java.io.Serializable;
import java.util.List;

public class EvsDeptGrade implements Serializable {

	/**
	 * 
	 */
	private static final long serialVersionUID = 1L;

	private String evEvPeriodId;

	private String evDeptId;

	private String evDeptGradeId;

	private String evDeptGradeName;

	private List lEvsEmpGrade;

	private List lEvsGradeRate;

	private int evEmpSum;

	public EvsDeptGrade() {
	}

	public EvsDeptGrade(String evDeptGradeId) {
	}

	public void addEvsEmpGrade(EvsEmpGrade evsEmpGrade) {
		if (evsEmpGrade != null) {
			this.lEvsEmpGrade.add(evsEmpGrade);
		}

	}

	/**
	 * @return 返回 evDeptGradeId。
	 */
	public String getEvDeptGradeId() {
		return evDeptGradeId;
	}

	/**
	 * @param evDeptGradeId
	 *            要设置的 evDeptGradeId。
	 */
	public void setEvDeptGradeId(String evDeptGradeId) {
		this.evDeptGradeId = evDeptGradeId;
	}

	/**
	 * @return 返回 evDeptGradeName。
	 */
	public String getEvDeptGradeName() {
		return evDeptGradeName;
	}

	/**
	 * @param evDeptGradeName
	 *            要设置的 evDeptGradeName。
	 */
	public void setEvDeptGradeName(String evDeptGradeName) {
		this.evDeptGradeName = evDeptGradeName;
	}

	/**
	 * @return 返回 evDeptId。
	 */
	public String getEvDeptId() {
		return evDeptId;
	}

	/**
	 * @param evDeptId
	 *            要设置的 evDeptId。
	 */
	public void setEvDeptId(String evDeptId) {
		this.evDeptId = evDeptId;
	}

	/**
	 * @return 返回 evEvPeriodId。
	 */
	public String getEvEvPeriodId() {
		return evEvPeriodId;
	}

	/**
	 * @param evEvPeriodId
	 *            要设置的 evEvPeriodId。
	 */
	public void setEvEvPeriodId(String evEvPeriodId) {
		this.evEvPeriodId = evEvPeriodId;
	}

	/**
	 * @return 返回 lEvEmpGrade。
	 */
	public List getLEvsEmpGrade() {
		return lEvsEmpGrade;
	}

	/**
	 * @param evEmpGrade
	 *            要设置的 lEvEmpGrade。
	 */
	public void setLEvsEmpGrade(List evsEmpGrade) {
		lEvsEmpGrade = evsEmpGrade;
	}

	/**
	 * @return 返回 lEvsGradeRate。
	 */
	public List getLEvsGradeRate() {
		return lEvsGradeRate;
	}

	/**
	 * @param evsGradeRate
	 *            要设置的 lEvsGradeRate。
	 */
	public void setLEvsGradeRate(List evsGradeRate) {
		lEvsGradeRate = evsGradeRate;
	}

	/**
	 * @return 返回 evEmpSum。
	 */
	public int getEvEmpSum() {
		return evEmpSum;
	}

	/**
	 * @param evEmpSum
	 *            要设置的 evEmpSum。
	 */
	public void setEvEmpSum(int evEmpSum) {
		this.evEmpSum = evEmpSum;
	}
}