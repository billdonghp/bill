package com.ait.sysparam;

public class PaSysparam extends Sysparam {
    private final static String default_paramName = "工资参数设置";
    private final static String default_description = "工资参数设置";
  
    public void initialized(){
    	this.setParamName(default_paramName);
    	this.setDescription(default_description);
    }

	/**
	 * 住房补贴操作结束日
	 * 
	 */
	private int tenementAllowDay  = 15;

	public int getTenementAllowDay() {
		return tenementAllowDay;
	}

	public void setTenementAllowDay(int tenement_allow_day) {
		tenementAllowDay = tenement_allow_day;
	}
}
