package com.ait.safe.bean;

public class PositionInfo {
	private String DISEASE;
	
	private String DISEASECODE;

	private String DISEASETYPE;
	
	private String JOB_DISEASE_TYPE_CODE;
	
	public String getJOB_DISEASE_TYPE_CODE() {
		return JOB_DISEASE_TYPE_CODE;
	}

	public void setJOB_DISEASE_TYPE_CODE(String job_disease_type_code) {
		JOB_DISEASE_TYPE_CODE = job_disease_type_code;
	}

	public String getDISEASE() {
		return DISEASE;
	}

	public void setDISEASE(String disease) {
		DISEASE = disease;
	}

	public String getDISEASETYPE() {
		return DISEASETYPE;
	}

	public void setDISEASETYPE(String diseasetype) {
		DISEASETYPE = diseasetype;
	}

	public String getDISEASECODE() {
		return DISEASECODE;
	}

	public void setDISEASECODE(String diseasecode) {
		DISEASECODE = diseasecode;
	}
}