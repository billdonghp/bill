function CheckForm(message)
{
	for(i=1;;i++){	
		
		if (document.getElementById("health_"+i) == null) {
		
			break;
		}
		
		// health information validate		
		if (document.getElementById("health_"+i) != null) {
				
				if (document.getElementById("health_"+i).checked){
				
					return true;
				}
		}			
		
	}
	//"请选择要删除的信息"
	alert(message);
	return false;
}