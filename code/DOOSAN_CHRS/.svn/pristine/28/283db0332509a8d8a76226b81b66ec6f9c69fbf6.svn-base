    <servlet-name>MenuTreeControl</servlet-name>
    <servlet-class>com.ait.web.MenuTreeControl</servlet-class>
  </servlet>


	<servlet-mapping>
		<servlet-name>commonXlsServlet</servlet-name>
		<url-pattern>/commonXlsServlet</url-pattern>
	</servlet-mapping>

	<servlet-mapping>
		<servlet-name>controlServlet</servlet-name>
