	<servlet>
		<servlet-name>Esy0105Control</servlet-name>
		<servlet-class>
			com.ait.sy.sy0105.servlet.Esy0105Control
		</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>UpdateControl</servlet-name>
		<servlet-class>com.ait.utils.UpdateControl</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>upImage</servlet-name>
