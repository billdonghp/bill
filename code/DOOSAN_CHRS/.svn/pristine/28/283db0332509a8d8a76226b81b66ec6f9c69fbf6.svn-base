		<servlet-class>
			com.ait.pa.servlet.PaControlServlet
		</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>gaControlServlet</servlet-name>
		<servlet-class>
			com.ait.ga.servlet.GaControlServlet
