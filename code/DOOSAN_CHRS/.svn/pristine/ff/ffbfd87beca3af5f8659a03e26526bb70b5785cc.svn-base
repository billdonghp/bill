function CheckForm() {
		if (document.sf.item_name.value == "") 	{
			alert ("\n 请填写工资项目名称！");
			document.sf.item_name.focus();
			return false;
		}

		if (document.sf.item_name.value.length > 25) 	{
			alert ("\n 工资项目名称字数不能大于25，请修改！");
			document.sf.item_name.focus();
			return false;
		}		

		if (document.sf.datatype.value == "") 	{
			alert ("\n 请填写数据类型！");
			document.sf.datatype.focus();
			return false;
		}

		if (document.sf.datatype.value.length > 25) 	{
			alert ("\n 数据类型字数不能大于25，请修改！");
			document.sf.datatype.focus();
			return false;
		}		
	

		if (document.sf.item_type.value == "") 	{
			alert ("\n 请填写项目类型！");
			document.sf.item_type.focus();
			return false;
		}

		if (document.sf.item_type.value.length > 25) 	{
			alert ("\n 项目类型字数不能大于25，请修改！");
			document.sf.item_type.focus();
			return false;
		}		

		if (document.sf.pricision.value == "") 	{
			alert ("\n 请填写精度！");
			document.sf.pricision.focus();
			return false;
		}

		if (document.sf.pricision.value.length > 25) 	{
			alert ("\n 精度字数不能大于25，请修改！");
			document.sf.pricision.focus();
			return false;
		}		

		return true;
}
