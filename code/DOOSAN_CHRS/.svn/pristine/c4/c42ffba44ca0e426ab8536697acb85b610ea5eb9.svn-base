	<insert id="insertApplyDetail" parameterClass="SimpleMap">
		<![CDATA[
	   insert into gm_eatery_confirm(
				eat_count_id,
				countm_type,
				count_fourdept,
				count_dept,
				count_date,
				count_num,
				cpny_id,
				flag
	           )
