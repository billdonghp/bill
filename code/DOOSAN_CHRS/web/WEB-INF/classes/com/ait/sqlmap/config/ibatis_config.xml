<sqlMapConfig>

	<!--  加载数据源属性文件  -->
	<properties resource="com/ait/sqlmap/config/dataSource.properties" />

	<!-- 配置SQL MAP CLIENT 参数 -->
	<settings cacheModelsEnabled="true" enhancementEnabled="true"
		lazyLoadingEnabled="true" maxRequests="128" maxSessions="80"
		maxTransactions="60" useStatementNamespaces="true" />
	
	<!-- 配置SQL MAP CLIENT 事物管理器 (JNDI方式获得) -->
	
	<transactionManager type="JDBC">
		<!--DICC数据库配置-->
		<dataSource type="JNDI">
			<property name="DataSource" value="java:comp/env/jdbc/EM2" />
		</dataSource>
	</transactionManager>


